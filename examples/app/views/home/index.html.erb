  <div>
    <% @types.each do |type| %>
    <span style="background-color: <%= @jpg_vibrant.try(type).hex %>; padding: 8px;">
      <%= type%> : <%= @jpg_vibrant.try(type).hex %>
    </span>
    <% end %>
  </div>

</div>