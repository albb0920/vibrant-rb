
<div>
  <h3>PNG</h3>
  <img src="<%= @png_path %>" height="200px"></img>
  <div>
    <% @types.each do |type| %>
    <span style="background-color: <%= @png_vibrant.try(type).hex %>; padding: 8px;">
      <%= type%> : <%= @png_vibrant.try(type).hex %>
    </span>
    <% end %>
  </div>

</div>

